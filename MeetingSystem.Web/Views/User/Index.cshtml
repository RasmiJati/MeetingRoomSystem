@model IEnumerable<MeetingRoom.Web.Models.User>
@{
    ViewData["Title"] = "Index";
}

<div class="container p-3">
    <div class=" row pt-4">
        <div class="col-6">
            <h2 class=" text-primary">User List</h2>
        </div>
    </div>
    <br /> <br />

</div>
<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Role</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="30%">@obj.Name</td>
                <td width="50%">@obj.Email</td>
                <td width="50%">@obj.Role</td>
            </tr>
        }
    </tbody>
</table>